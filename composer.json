{
	"name": "alfredoramos/hcaptcha",
	"type": "phpbb-extension",
	"description": "hCaptcha extension for phpBB",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-hcaptcha",
	"version": "0.2.0-dev",
	"time": "2021-09-14",
	"keywords": ["phpbb", "extension", "captcha", "hcatpcha", "anti-spam"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@yandex.com",
			"homepage": "https://alfredoramos.mx",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": "^7.1.3 || ^8.0.0",
		"composer/installers": "^1.6.0"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"pear/archive_tar": "^1.4.0"
	},
	"extra": {
		"display-name": "hCaptcha",
		"soft-require": {
			"phpbb/phpbb": "~3.3.0"
		},
		"version-check": {
			"host": "alfredoramos.mx",
			"directory": "/phpbb-extensions/versions",
			"filename": "hcaptcha.json",
			"ssl": true
		}
	}
}
